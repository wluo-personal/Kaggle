    "import pandas as pd\n",
    "\n",
    "PATH = '../../data/'\n",
    "\n",
    "g = pd.read_csv(PATH + 'logreg/logreg.csv')\n",
    "f = pd.read_csv(PATH + 'nblogreg/nblogreg.csv')\n",
    "\n",
    "l = ['toxic', 'obscene', 'insult', 'severe_toxic', 'threat', 'identity_hate']\n",
    "\n",
    "f[l] = (f[l]*2 + g[l]) / 3\n",
    "\n",
    "f.to_csv(PATH + 'twoe.csv', index=False)"
   ]
  }
